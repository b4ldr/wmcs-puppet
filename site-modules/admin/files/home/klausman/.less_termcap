export LESS_TERMCAP_mb=$(tput bold; tput setaf 6) # blink -> bold/cyan
export LESS_TERMCAP_md=$(tput bold; tput setaf 3) # bold -> bold/yellow
export LESS_TERMCAP_me=$(tput sgr0) # reset
export LESS_TERMCAP_mh=$(tput dim)  # ?
export LESS_TERMCAP_mr=$(tput rev)  # ?
export LESS_TERMCAP_se=$(tput rmso; tput sgr0) # stop standout
# Start standout, usually reverse video
export LESS_TERMCAP_so=$(tput bold; tput setab 4; tput setaf 7) # cyan
export LESS_TERMCAP_ue="$(tput sgr0)" # stop underline
export LESS_TERMCAP_us="$(tput bold)" # underline -> bold/white
export LESS_TERMCAP_ZN=$(tput ssubm) #?
export LESS_TERMCAP_ZO=$(tput ssupm) #?
export LESS_TERMCAP_ZV=$(tput rsubm) #?
export LESS_TERMCAP_ZW=$(tput rsupm) #?
