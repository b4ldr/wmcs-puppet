[Unit]
Description=Varnish frontend TLS ciphersuite inspector
After=varnish-frontend.service
Requires=varnish-frontend.service

[Service]
ExecStart=/usr/local/bin/varnishtlsinspector --varnishd-instance-name frontend
Restart=always
RestartSec=5s
SyslogIdentifier=%N

[Install]
WantedBy=multi-user.target
