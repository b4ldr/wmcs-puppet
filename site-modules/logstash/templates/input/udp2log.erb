input {
  # MediaWiki log stream forwarded by udp2log
  udp {
    type => "udp2log"
    port => <%= @port %>
<%- unless @plugin_id.empty? -%>
    id   => "<%= @plugin_id %>"
<%- end -%>
<%- if @tags -%>
    tags => ["<%= @tags.join('", "') %>", "deprecated-input"]
<%- else -%>
  tags => [ "deprecated-input" ]
<%- end -%>
  }
}
