# modules/dumps/files/web/htmldumps/nginx.conf
##############################

server {
   listen 80;
   listen [::]:80;

   server_name <%= @htmldumps_server %>;

   root /srv/www;

   location / {
      index index.html index.htm;
      autoindex on;
      charset utf-8;
   }
}
